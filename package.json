{
	"name": "@davidscicluna/component-library",
	"version": "2.0.1",
	"repository": "https://github.com/DavidScicluna/component-library.git",
	"author": {
		"name": "David Scicluna",
		"github": "https://github.com/DavidScicluna",
		"website": "https://davidscicluna.com"
	},
	"type": "module",
	"main": "./build/index.umd.js",
	"module": "./build/index.es.js",
	"types": "./build/index.d.ts",
	"files": [
		"build"
	],
	"exports": {
		".": {
			"import": "./build/index.es.js",
			"require": "./build/index.umd.js",
			"types": "./build/index.d.ts"
		},
		"./build/style.css": "./build/style.css"
	},
	"sideEffects": [
		"**/*.css"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"scripts": {
		"lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
		"format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json}\" --config ./.prettierrc",
		"cleanup": "rm -rf node_modules && rm -rf build && rm -rf types",
		"prebuild": "bun run cleanup && bun install && bun run lint && bun run format",
		"build:types": "tsc -project ./tsconfig.json",
		"build:vite": "tsc && vite build",
		"build:paths": "tsc-alias",
		"build:tailwind": "bun run tailwindcss -o ./src/common/styles/tailwind.css --minify",
		"build:cl": "sudo bun run prebuild && sudo bun run build:vite",
		"publish": "npm login && npm publish --access public",
		"storybook": "storybook dev --no-open -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@emotion/css": "^11.11.2",
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@floating-ui/react": "^0.26.1",
		"classnames": "^2.3.2",
		"color2k": "^2.0.2",
		"compressorjs": "^1.2.1",
		"fast-sort": "^3.4.0",
		"framer-motion": "^10.16.12",
		"lodash-es": "^4.17.21",
		"micro-memoize": "^4.1.2",
		"react": "latest",
		"react-cool-inview": "^3.0.1",
		"react-dom": "latest",
		"react-loader-spinner": "^6.1.6",
		"rooks": "^7.14.1",
		"tailwind-scrollbar-hide": "^1.1.7",
		"tailwindcss": "^3.3.6",
		"usehooks-ts": "^2.15.1",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.23.5",
		"@babel/preset-env": "^7.23.5",
		"@babel/preset-react": "^7.22.15",
		"@babel/preset-typescript": "^7.23.0",
		"@emotion/babel-plugin": "^11.11.0",
		"@emotion/babel-preset-css-prop": "^11.11.0",
		"@esbuild-plugins/node-globals-polyfill": "^0.2.3",
		"@esbuild-plugins/node-modules-polyfill": "^0.2.2",
		"@fontsource/material-icons": "^5.0.13",
		"@fontsource/material-icons-outlined": "^5.0.5",
		"@fontsource/material-icons-two-tone": "^5.0.5",
		"@fontsource/roboto-mono": "^5.0.5",
		"@fontsource/roboto-slab": "^5.0.5",
		"@fontsource/work-sans": "^5.0.5",
		"@storybook/addon-a11y": "^7.6.17",
		"@storybook/addon-essentials": "^7.6.17",
		"@storybook/addon-interactions": "^7.6.17",
		"@storybook/addon-links": "^7.6.17",
		"@storybook/addon-styling": "^1.3.7",
		"@storybook/react": "^7.6.17",
		"@storybook/react-webpack5": "^7.6.17",
		"@types/classnames": "^2.3.1",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.11.20",
		"@types/react": "latest",
		"@types/react-dom": "latest",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"@vitejs/plugin-react": "^4.2.1",
		"ajv": "^7",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.3",
		"babel-preset-react-app": "^10.0.1",
		"eslint": "^8.57.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-import-path": "^0.0.2",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.0",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"storybook": "^7.6.17",
		"tsc-alias": "^1.8.8",
		"tsconfig-paths-webpack-plugin": "^4.1.0",
		"typescript": "^5.3.3",
		"utility-types": "^3.10.0",
		"vite": "^5.1.4",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-dts": "^3.7.3",
		"vite-plugin-lib-inject-css": "^1.3.0",
		"vite-tsconfig-paths": "^4.3.1"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"typescript": "^5.3.3",
		"tailwindcss": "^3.3.3"
	},
	"license": "MIT"
}
