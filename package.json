{
	"name": "@davidscicluna/component-library",
	"version": "1.0.2",
	"repository": "https://github.com/DavidScicluna/component-library.git",
	"author": {
		"name": "David Scicluna",
		"github": "https://github.com/DavidScicluna",
		"website": "https://davidscicluna.com"
	},
	"main": "build/index.js",
	"module": "build/index.esm.js",
	"types": "build/index.d.ts",
	"files": [
		"build"
	],
	"scripts": {
		"lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
		"format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json}\" --config ./.prettierrc",
		"cleanup": "rimraf node_modules",
		"prebuild": "bun run cleanup && bun install && bun run lint && bun run format && rimraf build",
		"build:cl": "bun build ./src/index.ts --outdir ./build --target=node --format esm --minify",
		"build:tailwind": "bun tailwindcss -o build.css --minify",
		"build": "bun run prebuild && bun run build:cl && bun run build:tailwind",
		"publish": "bun npm login && bun npm publish --access public",
		"storybook": "storybook dev --no-open -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
